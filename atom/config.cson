  editor:
    preferredLineLength: 100
".elixir.source":
  editor:
    preferredLineLength: 100
".elixir.text":
  editor:
    preferredLineLength: 100
".haml.text":
  editor:
    preferredLineLength: 100
".hamlc.text":
  editor:
    preferredLineLength: 100
".handlebars.html.text":
  editor:
    preferredLineLength: 100
".ini.source":
